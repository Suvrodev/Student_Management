        <Label x:Name="label" Content="Your Information" VerticalAlignment="Top" Height="40" Width="518" FontWeight="Bold" FontSize="15" Background="#FFAAD352" HorizontalAlignment="Center" HorizontalContentAlignment="Center" Margin="0,0,-1,0" Grid.ColumnSpan="2"/>
        <TextBlock x:Name="for_id_Copy1" HorizontalAlignment="Left" Margin="0,103,0,0" TextWrapping="Wrap" Text="Message from dean" VerticalAlignment="Top" Width="131" Background="#FF53C9D4" Height="21" RenderTransformOrigin="1.269,2.737" FontSize="14" Grid.Column="1"/>
